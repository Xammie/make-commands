{
    "name": "xammie/make-commands",
    "description": "Extended make commands for laravel",
    "keywords": [
        "Xammie",
        "laravel",
        "make-commands"
    ],
    "homepage": "https://github.com/xammie/make-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Hoogenbosch",
            "email": "make-commands@hoogenbos.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Xammie\\MakeCommands\\": "src",
            "Xammie\\MakeCommands\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xammie\\MakeCommands\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xammie\\MakeCommands\\MakeCommandsServiceProvider"
            ],
            "aliases": {
                "MakeCommands": "Xammie\\MakeCommands\\Facades\\MakeCommands"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
